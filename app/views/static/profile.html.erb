<div class="container-fluid">
  <%= render "account_nav" %>

  <%= render "category_nav" %>

        <div class="row profile" id="ajax-profile">

            <%= render 'profile_page' %>

          <div class="col-md-7 profile-stories">
            <h1 class="underline">My Stories</h1>
            <% @my_articles.each do |article| %>
                <div class="row underline">
                  <div class="col-md-1">
                    <img src="<%= image_url "icon_tech.png" %>" class="badgeicon">
                  </div>
                  <div class="col-md-10">
                    <h3><%= article.title %></h3>
                    <p class="byline"><%= article.created_at %></p>
                  </div>
                  <div class="col-md-1 article-edit pull-right">
                    <img src="<%= image_url "edit_pencil.png" %>" class="pencil">
                  </div>
                </div>
            <% end %>
          </div>
        </div>

  <%= render "ourfooter" %>
</div>
